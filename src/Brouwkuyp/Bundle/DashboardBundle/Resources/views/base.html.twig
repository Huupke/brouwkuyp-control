<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Brouwkuyp-control{% endblock %}</title>
    {#<link href="{{ asset('css/bootstrap.css') }}" rel="stylesheet" media="screen">#}
    {#'bundles/brouwkuypdashboard/less/bootstrap.less'#}
    {% stylesheets
        '../src/Brouwkuyp/Bundle/DashboardBundle/Resources/public/less/bootstrap.less'
    %}
    <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
    <div class="container-fluid">
        {% block content %}{% endblock %}
    </div>
    <script src="{{ asset('js/jquery.js') }}"></script>
    <script src="{{ asset('js/bootstrap.js') }}"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/sockjs-client/0.3.4/sockjs.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/stomp.js/2.3.3/stomp.min.js"></script>
    {% javascripts
        'bundles/brouwkuypdashboard/js/highcharts.js'
        'bundles/brouwkuypdashboard/js/init.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
</body>
</html>
