{
    "name": "brouwkuyp/service-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides functionality of handling service specific functionality",
    "keywords": ["mqtt", "amqp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Evert Harmeling",
            "email": "evertharmeling@gmail.com"
        },
        {
            "name": "Luuk van Hal",
            "email": "lvanhal@gmail.com"
        }
    ],
    "require": {
        "symfony/config": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "symfony/yaml": "~2.4",
        "doctrine/common": "~2.4",
        "videlalvaro/php-amqplib": "2.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "psr-0": { "Brouwkuyp\\Bundle\\ServiceBundle": "" }
    },
    "target-dir": "Brouwkuyp/Bundle/ServiceBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
